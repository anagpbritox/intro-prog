#begin_inputs

letras = ['a', 'r', 's', 't',"f", 'u',"r","h"]
palavra = 'gigi'

#end_inputs

def advinhou_palavra(letras, palavra):
    for letra in palavra:
        if letra not in letras:
            return False
    return True
print(advinhou_palavra(letras, palavra))